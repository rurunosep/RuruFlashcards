    "url": "git+https://github.com/rurunosep/RuruFlashcards.git"
  },
  "author": "Kevin Hernandez-Rives",
  "dependencies": {
    "@google-cloud/text-to-speech": "^3.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
